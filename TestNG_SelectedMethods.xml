<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Suite">
	<listeners>
		<listener
			class-name="amazon_nagp_automation.listener.TestListener"></listener>
	</listeners>
	<test name="Amazon NAGP Smoke Suite">
		<classes>
			<class name="amazon_nagp_automation.testclass.SmokeTest">
				<methods>
					<include name="verifyChangeLanguage"></include>
					<include name="verifyAmazonPrime"></include>
				</methods>
			</class>
		</classes>
	</test>
</suite>